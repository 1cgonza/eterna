{
  "name": "eterna",
  "version": "0.0.1",
  "author": "Juan Camilo González Jiménez <info@juancgonzalez.com> (http://juancgonzalez.com)",
  "description": "An interactive data driven animation .",
  "main": "server.js",
  "scripts": {
    "dev:pack": "webpack-dev-server --config webpack.dev.js",
    "dev:server": "wait-on http://localhost:9000/ && node server.js",
    "dev": "concurrently --kill-others \"yarn run dev:pack\" \"yarn run dev:server\"",
    "start": "webpack --config webpack.prod.js && node server.js"
  },
  "dependencies": {
    "express": "^4.19.2",
    "sentiment": "^5.0.2",
    "socket.io": "^2.5.0",
    "twit": "^2.2.11"
  },
  "repository": {
    "url": "https://github.com/1cgonza/eterna"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "wait-on": "^4.0.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
